 <BASES>
  <BASE  name="Base de Règles">
    <Inhibited> no </Inhibited>
  </BASE>
  <BASE  name="Base Règle Incoherence">
    <PartOf> Base de Règles </PartOf>
    <Inhibited> yes </Inhibited>
    <RULE  name="Incoherence Arret">
     <Param> $1:Véhicule </Param>
     <Param> $2:Véhicule </Param>
     <Param> $3:Sinistre </Param>
     <If>
      $1§c1=vrai
      $2§c1=vrai
      $1#$2
     </If>
     <Then>
      Ajouter($3§saisie cohérente=faux)
      Message( "saisie incoherente. Les deux vehicules sont a l'arret" )
     </Then>
    </RULE>
    <RULE  name="Incoherence Arret1">
     <Param> $1:Véhicule </Param>
     <Param> $3:Sinistre </Param>
     <If>
      $1§c2=vrai
      $1§c3=vrai
      $1#$2
     </If>
     <Then>
      Ajouter($3§saisie cohérente=faux)
      Message( "saisie incoherente." )
     </Then>
    </RULE>
    <RULE  name="Incoherence Sens">
     <Param> $1:Véhicule </Param>
     <Param> $2:Véhicule </Param>
     <Param> $3:Sinistre </Param>
     <If>
      $1§c8=vrai
      $2§c8=vrai
      $1#$2
     </If>
     <Then>
      Ajouter($3§saisie cohérente=faux)
      Message( "saisie incoherente. Les deux vehicules disent s'etre rentré dedans par derrière" )
      Arrêt
     </Then>
    </RULE>
    <RULE  name="nb cases chochées">
     <Param> $1:Véhicule </Param>
     <Param> $2:Sinistre </Param>
     <If>
      $1§nb cases cochées&lt;>$1§compteur de cases
     </If>
     <Then>
      Ajouter($2§saisie cohérente=faux)
      Message( "saisie incoherente. Mauvais nombre de cases cochées" )
      Arrêt
     </Then>
    </RULE>
  </BASE>
  <BASE  name="Base Calcul Torts">
