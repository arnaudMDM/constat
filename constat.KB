    <Inhibited> yes </Inhibited>
    <RULE  name="faute grave reculait">
     <Param> $1:Véhicule </Param>
     <Param> $2:Véhicule </Param>
     <If>
      $1§c14=vrai
      $2§c4=faux
      $2§c14=faux
      $2§c17=faux
      $1#$2
     </If>
     <Then>
      Ajouter($1§torts=100)
      Ajouter($2§torts=0)
